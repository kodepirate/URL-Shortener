{
    "name": "URL Shortener",
    "version": "0.1",
    "description": "Create a tiny URL",
    "author": "kodepirate",
    "manifest_version": 2,

    "icons": {
        "16": "logo16.png",
        "48": "logo48.png"
    },

    "browser_action": {
        "default_popup": "popup.html"
    },

    "options_ui": {
        "page": "options.html",
        "open_in_tab": false
    },

    "permissions": [
        "storage"
    ]
}